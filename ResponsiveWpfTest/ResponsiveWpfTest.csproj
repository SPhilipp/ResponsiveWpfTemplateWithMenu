<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Images\copago-logo_black.png" />
    <None Remove="Assets\Images\Icons\home_page.png" />
    <None Remove="Assets\Images\Icons\img_setting.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AW.FontAwesome.WPF" Version="6.3.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Images\copago-logo_black.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\Images\Icons\home_page.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\Images\Icons\img_setting.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
